@{
    ViewBag.Title = "Entri Data Baru IUP OP Khusus Pengangkutan & Penjualan Batubara";
    Layout = "~/Areas/AngkutJual/Views/Shared/_LayoutAngkutJualTransaction.cshtml";
}
<style>
    #companyNameSumberSkAwal{text-transform:uppercase}
    #NmPerusahaan{text-transform:uppercase}
    #shareHolderName{text-transform:uppercase}
    #commisionerName{text-transform:uppercase}
    #directorName{text-transform:uppercase}
    #companyNameSumberSkAwal11{text-transform:uppercase}
    #companyNameSumberSkAwal{text-transform:uppercase}
    #sumberPerpanjanganName{text-transform:uppercase}
</style>

<div ng-app="iupop" ng-controller="iupop-controller">
    <!-- BEGIN PAGE CONTENT INNER -->
    <div class="page-content-inner">
        <div class="row">
            <div class="col-md-12">
                <!-- BEGIN VALIDATION STATES-->
                <div class="portlet light portlet-fit portlet-form box-shadow background-siluet">
                    <div class="portlet-title">
                        <ul class="page-breadcrumb breadcrumb">
                            <li>
                                <a href="#">Home</a>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <a href="#">Pengangkutan dan Penjualan</a>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <a href="@Url.Action("Index","iupopangkutjuallist", new { Area = "AngkutJual" })">Semua Data</a>
                                <i class="fa fa-circle"></i>
                            </li>
                            <li>
                                <span>Memasukan Data</span>
                            </li>
                        </ul>
                        <div class="row">
                            <div class="col-sm-12">
                                <h4 class="title">
                                    <i class="fa fa-pencil-square-o font-red"></i>
                                    <span class="font-red sbold">
                                        Data IUP OP Khusus Pengangkutan & Penjualan Batubara
                                        <input type="hidden" id="idx" value="@ViewBag.idx" />
                                    </span>
                                </h4>
                            </div>
                        </div>
                    </div>
                    <div class="portlet-body" style="overflow:hidden;">
                        <!-- BEGIN FORM-->
                        <div role="form">
                            <div class="form-body">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="tabbable-custom nav-justified">
                                            <ul class="nav nav-tabs nav-justified">
                                                <li class="active">
                                                    <a href="#tab_1_0" data-toggle="tab">
                                                        General Information
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#tab_1_1" data-toggle="tab">
                                                        Pemegang Saham
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#tab_1_2" data-toggle="tab">
                                                        Komisaris
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="#tab_1_3" data-toggle="tab">
                                                        Direksi
                                                    </a>
                                                </li>
                                            </ul>
                                            <div class="tab-content bg-grey bg-font-grey">
                                                <!--start general information-->
                                                <div class="tab-pane fade active in" id="tab_1_0" style="overflow:hidden;">
                                                    <!--<div class="form-body" style="overflow:hidden;">-->
                                                    <div class="col-md-6">
                                                        <div class="form-group has-success">
                                                            <label for="NmPerusahaan">Nama Perusahaan</label>
                                                            <input type="text" class="form-control" name="nmperusahaan" id="NmPerusahaan"
                                                                   ng-model="companyModel.name">
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="NoTlpn">No Telepon</label>
                                                            <input type="text" class="form-control" name="notlpn" id="NoTlpn"     
                                                                   placeholder="No. Telepon"
                                                                   ng-model="companyModel.telp">
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="NoTlpn">No Fax</label>
                                                            <input type="text" class="form-control" name="fax" id="fax"
                                                                   placeholder="No. Fax"
                                                                   ng-model="companyModel.fax">
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="Email_Website">Email </label>
                                                            <input type="email" class="form-control" name="email" id="Email"
                                                                   maxlength="35"
                                                                   placeholder="Email"
                                                                   ng-model="companyModel.email">
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="NpWp">NPWP</label>
                                                            <input type="text" class="form-control" name="npwp" id="NpWp"
                                                                   maxlength="21"
                                                                   data-mask="99.999.999.9-999.999"
                                                                   placeholder="NPWP"
                                                                   ng-model="companyModel.npwp">
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="Address">Alamat Perusahaan</label>
                                                            <textarea class="form-control" name="alamat" rows="3"
                                                                      placeholder="Alamat Perusahaan"
                                                                      ng-model="companyModel.address"></textarea>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="form-group has-success">
                                                            <label for="NoUrutBerkas">Nomor Urut Berkas</label>
                                                            <input type="text" class="form-control" name="nourutberkas" id="NoUrutBerkas"
                                                                   ng-model="companyModel.nourutberkas">
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="NoMobile">Handphone Contact Person</label>
                                                            <input type="text" class="form-control" name="NoMobile" id="NoMobile"
                                                                   value="+62"
                                                                   placeholder="No. HP (Diawali Dengan +62)"
                                                                   ng-model="companyModel.mobile">
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="NoMobile">Nama Contact Person</label>
                                                            <input type="text" class="form-control" name="namacp" id="namacp"
                                                                   placeholder="Nama CP"
                                                                   ng-model="companyModel.namacp">
                                                        </div>
                                                        <div class="form-group has-success">
                                                                <label for="website">Website </label>
                                                                <input type="text" class="form-control" name="website" id="website"
                                                                       placeholder="Website"
                                                                       ng-model="companyModel.website">
                                                            </div>
                                                        <div class="form-group has-success">
                                                            <label for="form_control_1">Status Izin</label>
                                                            <select class="form-control" name="stsizin" id="stsizin"
                                                                    ng-model="companyModel.statusizin">
                                                                <option value="1">Aktif</option>
                                                                <option value="0">Non Aktif</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-group has-success">
                                                            <label for="remark">Keterangan</label>
                                                            <textarea class="form-control" name="keterangan" rows="3"
                                                                      placeholder="Keterangan"
                                                                      ng-model="companyModel.remark"></textarea>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-12">
                                                        <div class="form-actions no-padding">
                                                            <a class="btn green-sharp red-stripe" ng-click="saveCompanyGeneralInfo()">
                                                                <i class="fa fa-save"></i>
                                                                Simpan
                                                            </a>
                                                        </div>
                                                    </div>

                                                    <!--</div>-->
                                                </div>
                                                <!--end general information-->
                                                <!--start table pemegang saham-->
                                                <div class="tab-pane fade" id="tab_1_1">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-bordered table-hover">
                                                            <thead class="bg-green-sharp bg-font-green-sharp">
                                                                <tr>
                                                                    <th class="nama"> Nama </th>
                                                                    <th> Status Kewarganegaraan </th>
                                                                    <th> Jumlah (Lembar) </th>
                                                                    <th> Nilai Saham (Rupiah)</th>
                                                                    <th> &nbsp; </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="item in shareHolders">
                                                                    <td> {{item.Name}} </td>
                                                                    <td>
                                                                        <span ng-show="item.StatusWnBh == 'WNI'">WNI</span>
                                                                        <span ng-show="item.StatusWnBh == 'WNA'">WNA</span>
                                                                        <span ng-show="item.StatusWnBh == 'PMDN'">PMDN</span>
                                                                        <span ng-show="item.StatusWnBh == 'PMA'">PMA</span>
                                                                    </td>
                                                                    <td> {{item.TotalStock | currency : "" : 0}} </td>
                                                                    <td> {{item.Currency | currency: "": 0}} </td>
                                                                    <td class="text-center">
                                                                        <a href="javascript:void(0)"
                                                                           ng-click="editShareHolderClick(item.ID)"
                                                                           class="btn btn-xs btn-outline">
                                                                            <i class="fa fa-pencil"></i>
                                                                        </a>
                                                                        |
                                                                        <a href="javasript:void(0)"
                                                                           ng-click="deleteShareholder(item.ID)"
                                                                           class="btn btn-xs btn-outline">
                                                                            <i class="fa fa-trash"></i>
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="nama">
                                                                        <input class="form-control form-filter input-sm"
                                                                               id="shareHolderName"
                                                                               name="nama" type="text" ng-model="shareholderModel.name">
                                                                    </td>
                                                                    <td>
                                                                        <select class="form-control" ng-model="shareholderModel.statuswnbh">
                                                                            <option value="WNI">WNI</option>
                                                                            <option value="WNA">WNA</option>
                                                                            <option value="PMDN">PMDN</option>
                                                                            <option value="PMA">PMA</option>
                                                                        </select>
                                                                    </td>
                                                                    <td>
                                                                        <input class="form-control form-filter input-sm"                                                                               
                                                                               name="jumlah" type="number" ng-model="shareholderModel.jumlah">
                                                                    </td>
                                                                    <td>
                                                                        <input class="form-control form-filter input-sm"
                                                                               name="currency" type="number" ng-model="shareholderModel.currency">
                                                                    </td>
                                                                    <td>
                                                                        <a href="javascript:void(0)"
                                                                           ng-click="saveShareholder()"
                                                                           class="btn green-sharp btn-outline btn-block red-stripe">
                                                                            <i class="fa fa-save"></i> Simpan
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>                                                   
                                                </div>
                                                <!--end table pemegang saham-->
                                                <!--start table komisaris-->
                                                <div class="tab-pane fade" id="tab_1_2">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-bordered table-hover">
                                                            <thead class="bg-green-sharp bg-font-green-sharp">
                                                                <tr>
                                                                    <th class="nama"> Nama </th>
                                                                    <th> NPWP </th>
                                                                    <th> Status Kewarganegaraan </th>
                                                                    <th> &nbsp; </th>

                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="item in Commisioners">
                                                                    <td class="nama"> {{item.Name}} </td>
                                                                    <td> {{item.NPWP}} </td>
                                                                    <td>
                                                                        <span ng-show="item.Status == 'WNI'">WNI</span>
                                                                        <span ng-show="item.Status == 'WNA'">WNA</span>
                                                                    </td>
                                                                    <td class="text-center">
                                                                        <a href="javascript:void(0)"
                                                                           ng-click="editCommisionerClick(item.ID)"
                                                                           class="btn btn-xs btn-outline">
                                                                            <i class="fa fa-pencil"></i>
                                                                        </a>
                                                                        |
                                                                        <a href="javasript:void(0)"
                                                                           ng-click="deleteCommisioner(item.ID)"
                                                                           class="btn btn-xs btn-outline">
                                                                            <i class="fa fa-trash"></i>
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="nama">
                                                                        <input class="form-control form-filter input-sm"
                                                                               name="nama" type="text"
                                                                               id="commisionerName"
                                                                               ng-model="CommisionerModel.Name" />
                                                                    </td>
                                                                    <td>
                                                                        <input class="form-control form-filter input-sm npwpCommissioner" name="NPWP" id="NPWPCommissioner"
                                                                               data-mask="99.999.999.9-999.999"
                                                                               type="text"
                                                                               ng-model="CommisionerModel.NPWP" />
                                                                        @*@Html.Kendo().MaskedTextBox().Name("NPWPCommissioner").Mask("00.000.000.0-000.000").HtmlAttributes(new {@ng_model = "CommisionerModel.NPWP", @placeholder = "NPWP", @class = "form-control" })*@
                                                                    </td>
                                                                    <td>
                                                                        <select class="form-control"
                                                                                ng-model="CommisionerModel.Status"
                                                                                id="CommisionerStatus">
                                                                            <option value="WNI">WNI</option>
                                                                            <option value="WNA">WNA</option>
                                                                        </select>
                                                                    </td>
                                                                    <td>
                                                                        <a href="javascript:void(0)"
                                                                           ng-click="saveCommisioner()"
                                                                           class="btn green-sharp btn-outline btn-block red-stripe">
                                                                            <i class="fa fa-save"></i> Simpan
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                                <!--end table komisaris-->
                                                <!--start table direksi-->
                                                <div class="tab-pane fade" id="tab_1_3">
                                                    <div class="table-responsive">
                                                        <table class="table table-striped table-bordered table-hover">
                                                            <thead class="bg-green-sharp bg-font-green-sharp">
                                                                <tr>
                                                                    <th class="nama"> Nama </th>
                                                                    <th> NPWP </th>
                                                                    <th> Status Kewarganegaraan </th>
                                                                    <th> &nbsp; </th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="item in Directors">
                                                                    <td class="nama"> {{item.Name}} </td>
                                                                    <td> {{item.NPWP}}</td>
                                                                    <td>
                                                                        <span ng-show="item.Status == 'WNI'">WNI</span>
                                                                        <span ng-show="item.Status == 'WNA'">WNA</span>
                                                                    </td>
                                                                    <td class="text-center">
                                                                        <a href="javascript:void(0)"
                                                                           ng-click="editDirectorClick(item.ID)"
                                                                           class="btn btn-xs btn-outline">
                                                                            <i class="fa fa-pencil"></i>
                                                                        </a>
                                                                        |
                                                                        <a href="javasript:void(0)"
                                                                           ng-click="deleteDirector(item.ID)"
                                                                           class="btn btn-xs btn-outline">
                                                                            <i class="fa fa-trash"></i>
                                                                        </a>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td class="nama">
                                                                        <input class="form-control form-filter input-sm"
                                                                               name="nama" type="text"
                                                                               id="directorName"
                                                                               ng-model="DirectorModel.Name" />
                                                                    </td>
                                                                    <td>
                                                                        <input class="form-control form-filter input-sm" name="ket"
                                                                               data-mask="99.999.999.9-999.999"
                                                                               type="text"
                                                                               id="directorNpwp"
                                                                               ng-model="DirectorModel.NPWP" />
                                                                    </td>
                                                                    <td>
                                                                        <select class="form-control"
                                                                                ng-model="DirectorModel.Status"
                                                                                id="DirectorStatus">
                                                                            <option value="WNI">WNI</option>
                                                                            <option value="WNA">WNA</option>
                                                                        </select>
                                                                    </td>
                                                                    <td>
                                                                        <a href="javascript:void(0)"
                                                                           ng-click="saveDirector()"
                                                                           class="btn green-sharp btn-outline btn-block red-stripe">
                                                                            <i class="fa fa-save"></i> Simpan
                                                                        </a>

                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                                <!--end table direksi-->
                                            </div>
                                        </div>
                                        <div class="space"></div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div id="modalUploadFile"
                                             class="modal fade"
                                             tabindex="-1" data-backdrop="static" data-keyboard="false">
                                            <div class="modal-header">
                                                <button type="button"
                                                        class="close"
                                                        data-dismiss="modal"
                                                        aria-hidden="true"></button>
                                                <h4 class="modal-title">Upload File {{modelUpload.title}}</h4>
                                            </div>
                                            <div class="modal-body">
                                                <div class="row">
                                                    <input type="hidden"
                                                           id="idActiveTable"
                                                           ng-model="modelUpload.id" />
                                                    <input name="files" id="files" type="file" />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!--start row accordion-->
                                <div class="row">
                                    <div class="col-md-12">
                                        <!--start accordion-->
                                        <div class="panel-group accordion" id="FormEvaluasiRKAB">
                                            <!--start accordion Sk awal-->
                                            <div class="panel border-red">
                                                <div class="panel-heading bg-red bg-font-red">
                                                    <h4 class="panel-title">
                                                        <a class="accordion-toggle accordion-toggle-styled" data-toggle="collapse"
                                                           data-parent="#FormEvaluasiRKAB" href="#DetailPelaporan">
                                                            Data Legalitas
                                                        </a>
                                                    </h4>
                                                </div>
                                                <div id="DetailPelaporan" class="panel-collapse collapse">
                                                    <div class="panel-body">
                                                        <h4>SK Awal</h4>
                                                        <!--start modal add Sk Iupop-->
                                                        <div id="addSkIupOp" class="modal container fade"
                                                             tabindex="-1" data-backdrop="static" data-keyboard="false">
                                                            <div class="modal-header">
                                                                <button type="button"
                                                                        class="close"
                                                                        data-dismiss="modal"
                                                                        aria-hidden="true"></button>
                                                                <h4 class="modal-title">Tambah Sumber</h4>
                                                            </div>
                                                            <div class="modal-body">
                                                                <div class="row">
                                                                    <input id="IdFirstSkSource" ng-model="FirstSkSourceModel.ID" hidden="hidden" />
                                                                    <div class="col-sm-6 col-md-4 col-md-offset-2">
                                                                        <div class="form-group has-success">
                                                                            <label for="NamaPerusahaanSkAwal">Nama Perusahaan</label>
                                                                            <input type="text"
                                                                                   id="companyNameSumberSkAwal11"
                                                                                   ng-model="FirstSkSourceModel.CompanyName"
                                                                                   class="form-control" />
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-sm-6 col-md-4">
                                                                        <div class="form-group has-success">
                                                                            <label for="VolumeSkAwal">Volume (MT)</label>
                                                                            <input type="number" class="form-control"
                                                                                   ng-model="FirstSkSourceModel.Volume" />
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                            <div class="modal-footer">
                                                                <button type="button"
                                                                        ng-show="!isEditSkAwalIupOp"
                                                                        class="btn green-sharp red-stripe"
                                                                        ng-click="simpanSkAwalIupOp()">
                                                                    <i class="fa fa-save"></i>
                                                                    Simpan
                                                                </button>
                                                                <button type="button"
                                                                        ng-show="isEditSkAwalIupOp"
                                                                        class="btn green-sharp red-stripe"
                                                                        ng-click="simpanEditSkAwalIupOp()">
                                                                    <i class="fa fa-save"></i>
                                                                    Simpan Perubahan
                                                                </button>
                                                                <button type="button" data-dismiss="modal" class="btn red">
                                                                    <i class="fa fa-close"></i>
                                                                    Batal
                                                                </button>
                                                            </div>
                                                        </div>
                                                        <!--end modal add Sk Iupop-->
                                                        <div class="row" ng-show="isAddSkAwal || isEditSkAwal">
                                                            <div class="form-body">
                                                                <div class="col-md-6">
                                                                    <div class="form-group has-success">
                                                                        <label for="JenisLaporan">No. Surat Permohonan</label>
                                                                        <input type="text" class="form-control" name="nosurat" id="NoSurat"
                                                                               ng-model="skAwalModel.nosurat" />
                                                                    </div>
                                                                    <div class="form-group has-success">
                                                                        <label for="NoSk">No. SK</label>
                                                                        <input type="text" class="form-control" name="nosk" id="NoSk"
                                                                               ng-model="skAwalModel.nosk" />
                                                                    </div>
                                                                    <div class="form-group has-success">
                                                                        <label>Masa Berlaku (Tahun)</label>
                                                                        <input type="text" class="form-control" ng-model="skAwalModel.jangkawaktu" id="masaberlaku" name="masaberlaku" />
                                                                    </div>
                                                                    <div class="form-group has-success">
                                                                        <label>Sertifikat CNC</label>
                                                                        <input type="text" class="form-control" ng-model="skAwalModel.sertifikatCnc" id="sertifikatCnc" name="sertifikatCnc" />
                                                                    </div>
                                                                </div>
                                                                <div class="col-md-6">
                                                                    <div class="form-group has-success">
                                                                        <label> Tanggal Surat Permohonan </label>
                                                                        <input class="form-control form-control-inline date-picker" size="16" type="text" value=""
                                                                               id="tanggalpermohonanskawal"
                                                                               data-date-format="dd/mm/yyyy"
                                                                               ng-model="skAwalModel.tanggalpermohonan" />

                                                                    </div>
                                                                    <div class="form-group has-success">
                                                                        <label> Tanggal SK {{skAwalModel.tglsk}} </label>
                                                                        <input class="form-control form-control-inline date-picker" type="text"
                                                                               id="tglskawal"
                                                                               data-date-format="dd/mm/yyyy"
                                                                               ng-model="skAwalModel.tglsk" />

                                                                    </div>
                                                                    <div class="form-group has-success">
                                                                            <label>Tanggal Izin Berakhir</label>
                                                                            <input class="form-control form-control-inline date-picker" size="16" type="text" value=""
                                                                                   id="tglberakhirskawal"
                                                                                   data-date-format="dd/mm/yyyy"
                                                                                   ng-model="skAwalModel.tglberakhir" disabled="disabled" />
                                                                        </div>
                                                                    <input type="hidden" class="form-control"
                                                                           ng-model="skAwalModel.skFile" />
                                                                </div>
                                                            </div>
                                                        </div>

                                                        <div class="text-right">
                                                            <a href="javascript:void(0)"
                                                               ng-show="isBtnAddSkAwalShowed && skAwal.length == 0"
                                                               ng-click="tambahSkAwalClick()"
                                                               class="btn green-sharp red-stripe">
                                                                <i class="fa fa-plus"></i> Tambah SK
                                                            </a>
                                                            <a href="javascript:void(0)"
                                                               ng-show="isBtnSaveSkAwalShowed && skAwal.length == 0"
                                                               ng-click="saveTambahEditSkAwalClick()"
                                                               class="btn green-sharp red-stripe">
                                                                <i class="fa fa-save"></i> Simpan
                                                            </a>
                                                            <a href="javascript:void(0)" ng-show="isBtnCancelSkAwalShowed && skAwal.length == 0"
                                                               ng-click="cancelTambahEditSkAwalClick()"
                                                               class="btn red">
                                                                <i class="fa fa-close"></i> Batal
                                                            </a>
                                                            <a href="javascript:void(0)"
                                                               ng-show="isEditSkAwal && skAwal.length > 0"
