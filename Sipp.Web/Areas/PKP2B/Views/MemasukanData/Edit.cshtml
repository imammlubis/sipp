
@{
    ViewBag.Title = "- Edit data";
    Layout = "~/Areas/PKP2B/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-md-12">
        <!-- BEGIN VALIDATION STATES-->
        <div class="portlet light portlet-fit portlet-form box-shadow background-siluet">
            <div class="portlet-title">
                <div class="caption">
                    <i class="icon-layers font-red"></i>
                    <span class="caption-subject font-red sbold uppercase">Memasukan Data PKP2B</span>
                </div>
            </div>
            <input type="hidden" id="idx" value="@ViewBag.idx" />
            <div ng-app="pkp2b" ng-controller="pkp2b-controller" class="portlet-body" style="overflow:hidden;">
                <div class="form-body">
                    <div class="tabbable-line">
                        <ul class="nav nav-tabs ">
                            <li class="active">
                                <a href="#tab_15_1" data-toggle="tab"> General Information </a>
                            </li>
                            <li>
                                <a href="#tab_15_2" data-toggle="tab"> Status Permodalan </a>
                            </li>
                        </ul>
                        <div class="tab-content">
                            <div class="tab-pane active" id="tab_15_1">
                                <!-- BEGIN FORM-->
                                <form name="userForm" data-toggle="validator" role="form">
                                    <div class="row no-margin">
                                        <div class="col-md-6">
                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Nama Perusahaan</label>
                                                    <input type="text" 
                                                           class="form-control uppercase"
                                                           ng-model="companyModel.nama"
                                                           tabindex="1"
                                                           name="namaPerusahaan" 
                                                           id="namaPerusahaan" 
                                                           required />
                                                </div>
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">NPWP</label>
                                                    <input type="text" class="form-control"
                                                           ng-model="companyModel.npwp"
                                                           maxlength="21"
                                                           tabindex="2"
                                                           data-mask="99.999.999.9-999.999"
                                                           name="npwp" id="npwp" required>
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="form-group has-success col-xs-12">
                                                    <label for="NewPass">Alamat</label>
                                                    <textarea class="form-control"
                                                              tabindex="3"
                                                              ng-model="companyModel.alamat" rows="5" required></textarea>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Telp</label>
                                                    <input type="text" ng-model="companyModel.telp"
                                                           class="form-control" name="ConfirmNew" id="ConfirmNew">
                                                </div>

                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Fax</label>
                                                    <input type="text" ng-model="companyModel.fax" class="form-control" name="ConfirmNew" id="ConfirmNew">
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="Email">Email</label>
                                                    <input type="email" ng-model="companyModel.email"
                                                           maxlength="35"
                                                           data-error="Bruh, that email address is invalid"
                                                           class="form-control" id="Email">
                                                    <div class="help-block with-errors"></div>
                                                </div>

                                                <div class="form-group has-success col-sm-6">
                                                    <label for="Website">Website</label>
                                                    <input type="text" ng-model="companyModel.website" class="form-control" id="Website">
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Contact Person (Nama)</label>
                                                    <input type="text" ng-model="companyModel.namacp" class="form-control uppercase" name="cpNama" id="cpNama">
                                                </div>
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Contact Person (HP)</label>
                                                    <input type="text" value="+62" ng-model="companyModel.hpcp" class="form-control" name="cpHp" id="cpHp">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12">
                                            <h3 class="bold">Data PKP2B</h3>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Generasi</label>
                                                    <select class="form-control" ng-model="dataPkp2bModel.generasi" name="generasi" id="generasi" required>
                                                        <option></option>
                                                        <option value="I">Generasi I</option>
                                                        <option value="II">Generasi II</option>
                                                        <option value="III">Generasi III</option>
                                                    </select>
                                                </div>

                                                <div class="form-group has-success col-sm-6" required>
                                                    <label for="ConfirmNew">Kode Wilayah</label>
                                                    <input type="text" class="form-control" ng-model="dataPkp2bModel.kodeWilayah" name="kodeWilayah" id="kodeWilayah" required>
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Luas Wilayah (Awal)</label>
                                                    <input type="number"
                                                           ng-model="dataPkp2bModel.luasWilayahAwal" id="luasWilayahAwal" class="form-control" required>
                                                </div>

                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Luas Wilayah (Dipertahankan)</label>
                                                    <input type="number"
                                                           ng-model="dataPkp2bModel.luasWilayahDipertahankan" id="luasWilayahDipertahankan" class="form-control" required>
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Tahapan Akhir</label>
                                                    <select class="form-control" ng-model="dataPkp2bModel.tahapanAkhir" name="tahapanAkhir" id="tahapanAkhir" required>                                                        
                                                        <option></option>
                                                        <option value="Penyelidikan Umum">Penyelidikan Umum</option>
                                                        <option value="Eksplorasi">Eksplorasi</option>
                                                        <option value="Studi Kelayakan">Studi Kelayakan</option>
                                                        <option value="Konstruksi">Konstruksi</option>
                                                        <option value="Operasi Produksi">Operasi Produksi</option>
                                                    </select>
                                                </div>  
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="perizinan">Perizinan</label>
                                                    <select class="form-control" 
                                                            ng-model="dataPkp2bModel.perizinan" 
                                                            required>
                                                        <option></option>
                                                        <option value="BUMN">BUMN</option>
                                                        <option value="IUPOPK">IUPOPK</option>
                                                    </select>
                                                </div>                                              
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">No Kontrak</label>
                                                    <input type="text" ng-model="dataPkp2bModel.noKontrak" required class="form-control" name="ConfirmNew" id="ConfirmNew">
                                                </div>
                                                <div class="form-group has-success col-sm-6">
                                                    <label>Tanggal Kontrak</label>
                                                    @*<input type="text" class="form-control form-control-inline date-picker" size="16"
                                required
                                id="tanggalKontrak"
                                data-date-format="dd/mm/yyyy"
                                ng-model="dataPkp2bModel.tanggalKontrak" />*@
                                                    <div class="input-group input-append date" id="tanggalKontrak">
                                                        <input type="text" ng-model="dataPkp2bModel.tanggalKontrak" 
                                                               class="form-control" data-date-format="dd/mm/yyyy"
                                                               id="tanggalKontrak"
                                                               name="tanggalKontraks" />
                                                        <span class="input-group-addon add-on">
                                                            <span class="glyphicon glyphicon-calendar">
                                                            </span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Lokasi (Provinsi)</label>
                                                    <input type="text" ng-model="dataPkp2bModel.provinsi" id="provinsi" class="form-control uppercase" required>
                                                </div>
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Lokasi (Kabupaten)</label>
                                                    <input type="text" ng-model="dataPkp2bModel.kabupaten" id="kabupaten" class="form-control uppercase" required>
                                                </div>
                                            </div>

                                            <div class="row">
                                                <div class="form-group has-success col-sm-6">
                                                    <label for="ConfirmNew">Jangka Waktu</label>
                                                    <input class="form-control" ng-model="dataPkp2bModel.jangkaWaktu" 
                                                           id="jangkaWaktu"
                                                           required type="number" value="" />
                                                </div>
                                                <div class="form-group has-success col-sm-6">
                                                    <label>Tanggal Berakhir</label>
                                                    @*<input class="form-control form-control-inline date-picker"
                                size="16" type="text"
                                id="tanggalBerakhir"
                                data-date-format="dd/mm/yyyy"
                                ng-model="dataPkp2bModel.tanggalBerakhir" />*@
                                                    <div class="input-group input-append date" id="tanggalBerakhir">
                                                        <input type="text" 
                                                               date-disabled="'true'" disabled
                                                               id="tanggalBerakhir"
                                                               ng-model="dataPkp2bModel.tanggalBerakhir"
                                                               class="form-control" data-date-format="dd/mm/yyyy"
                                                               name="tanggalBerakhir" />
                                                        <span class="input-group-addon add-on">
                                                            <span class="glyphicon glyphicon-calendar">
                                                            </span>
                                                        </span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-12 text-right">
                                            <button type="submit" class="btn green-sharp red-stripe" ng-click="SaveEditGeneralInformation()"><i class="fa fa-save"></i> Simpan</button>
                                            <div class="space"></div>
                                        </div>
                                        <div class="col-md-12">
                                            <div class="portlet box green">
                                                <div class="portlet-title">
                                                    <div class="caption">
                                                        History Data PKP2B
                                                    </div>
                                                    <div class="tools">
                                                        <a href="javascript:;" class="collapse" data-original-title="" title="" style="margin-right:5px;"> </a>
                                                    </div>
                                                </div>
                                                <div class="portlet-body">
                @(Html.Kendo().Grid<EduSpot.Entity.Tables.Pkp2b.HistoryDataPkp2b>()
                   .Name("gridHistory")
                   .EnableCustomBinding(true)
                   .Columns(columns =>
                   {
                       columns.Bound(p => p.ID).Hidden();
                       columns.Bound(p => p.CreatedDate).Title("Tanggal Diubah").Format("{0:dd/MM/yyyy HH:mm}").Width(170);
                       columns.Bound(p => p.Generasi).Title("Generasi").Width(223);
                       columns.Bound(p => p.KodeWilayah).Title("Kode Wilayah").Width(200);
                       columns.Bound(p => p.NoKontrak).Title("No. Kontrak").Width(200);
                       columns.Bound(p => p.LuasWilayahAwal).Title("Luas Wilayah(Awal)").Width(200);
                       columns.Bound(p => p.LuasWilayahDipertahankan).Title("Luas Wilayah (dipertahankan)").Width(200);
                       columns.Bound(p => p.Provinsi).Title("Provinsi").Width(200);
                       columns.Bound(p => p.Kabupaten).Title("Kabupaten").Width(200);
                       columns.Bound(p => p.TahapanAkhir).Title("Tahapan Akhir").Width(200);
                       columns.Bound(p => p.JangkaWaktu).Title("Jangka Waktu").Width(200);
                       columns.Bound(p => p.TanggalKontrak).Title("Tanggal Kontrak").Format("{0:dd/MM/yyyy}").Width(170);
                       columns.Bound(p => p.TanggalBerakhir).Title("Tanggal Berakhir").Format("{0:dd/MM/yyyy}").Width(170);

                   })
                   .Sortable()
                   .Pageable()
                   .Scrollable()
                   .Filterable()
                   .ColumnMenu()
                   .Resizable(resize => resize.Columns(true))
                   .DataSource(dataSource => dataSource
                       .Ajax()
                       .Read("ListHistoryDataPkp2b", "memasukandata", new { area = "pkp2b", id= ViewBag.idx })
                       .ServerOperation(true)
                    ))
                                                </div>
                                            </div>
                                        </div>

                                        
                                    </div>
                                </form>
                                <!-- END FORM-->

                                <div class="row no-margin">
                                    <div class="col-md-12">
                                        <div class="portlet box green">
                                            <div class="portlet-title">
                                                <div class="caption">
                                                    Data Akta
                                                </div>
                                                <div class="actions">
                                                    <div class="btn-group btn-group-solid">
                                                        <a ng-click="tambahDataAktaClick()"
                                                           data-target="#static" 
                                                           data-toggle="modal" type="button" 
                                                           class="btn red">
                                                            <i class="fa fa-plus"></i> Tambah Data
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="tools">
                                                    <a href="javascript:;" class="collapse" data-original-title="" title="" style="margin-right:5px;"> </a>
                                                </div>
                                            </div>
                                            <div class="portlet-body">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-bordered table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th colspan="3" class="text-center"> Akta Pendirian dan Perubahan </th>
                                                                <th colspan="2" class="text-center"> Pengesahan Kemenkumham </th>
                                                                <th rowspan="2" class="text-center"> File </th>
                                                                <th colspan="2" rowspan="2" class="text-center"> action </th>
                                                            </tr>
                                                            <tr>
                                                                <th> Judul Akta </th>
                                                                <th> No Akta </th>
                                                                <th> Tanggal Akta </th>
                                                                <th> No </th>
                                                                <th> Tanggal </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="item in dataAkta">
                                                                <th style="min-width:150px;"> &nbsp; {{item.JudulAkta}} </th><!-- judul akta-->
                                                                <th style="min-width:150px;"> &nbsp; {{item.NoAkta}}</th><!-- no akta-->
                                                                <th style="min-width:150px;"> &nbsp; {{item.TanggalAkta | formatDate | date:'dd/MM/yyyy'}}</th><!-- judul akta-->
                                                                <th style="min-width:150px;"> &nbsp; {{item.NoPengesahanKemenkumham}}</th><!-- no-->
                                                                <th style="min-width:150px;"> &nbsp; {{item.TanggalPengesahanKemenkumhan | formatDate | date:'dd/MM/yyyy'}}</th><!-- tanggal-->
                                                                <td>
                                                                    <a ng-show="item.File"
                                                                       href="../../../DocumentPkp2b/DataAkta/{{item.File}}"
                                                                       target="_blank">
                                                                        <i class="fa fa-paperclip" aria-hidden="true"></i>
                                                                    </a>
                                                                </td>
                                                                <td class="text-center">
                                                                    <a href="javascript:void(0)" ng-click="editDataAktaClick(item.ID)"><i class="fa fa-pencil font-blue" title="Edit"></i></a>
                                                                </td>
                                                                <td class="text-center">
                                                                    <a href="javascript:void(0)" ng-click="deleteDataAkta(item.ID)"><i class="fa fa-trash font-red" title="Delete"></i></a>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row no-margin">
                                    <div class="col-md-12">
                                        <div class="portlet box green">
                                            <div class="portlet-title">
                                                <div class="caption">
                                                    Tahap Kegiatan
                                                </div>
                                                <div class="actions">
                                                    <div class="btn-group btn-group-solid">
                                                        <a data-target="#static6" 
                                                           data-toggle="modal" 
                                                           ng-click="tambahTahapKegiatanClick()" 
                                                           class="btn red">
                                                            <i class="fa fa-plus"></i> Tambah Data
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="tools">
                                                    <a href="javascript:;" class="collapse" data-original-title="" title="" style="margin-right:5px;"> </a>
                                                </div>
                                            </div>
                                            <div class="portlet-body">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-bordered table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th style="min-width:150px;"> Tahap kegiatan </th>
                                                                <th style="min-width:200px;"> Luas Dipertahankan (HA) </th>
                                                                <th> Detail Tahap Kegiatan</th>
                                                                <th colspan="2"> action </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="item in dataTahapKegiatan">
                                                                <td style="min-width:150px;"> {{item.Tahap}} </td>
                                                                <td style="min-width:200px;"> {{item.LuasDipertahankan}} </td>
                                                                <td>
                                                                    <!-- table detail -->
                                                                    <table class="table table-striped table-bordered table-hover">
                                                                        <thead class="bg-green-soft bg-font-green-soft">
                                                                            <tr>
                                                                                <th style="min-width:190px;"> Tahap kegiatan / Blok </th>
                                                                                <th style="min-width:240px;"> Luas Dipertahankan / Blok (HA) </th>
                                                                                <th colspan="2" class="text-center">
                                                                                    <div class="btn-group btn-group-solid">
                                                                                        <a style="margin-right:0;" 
                                                                                           href="" 
                                                                                           data-target="#detailStatic6" 
                                                                                           data-toggle="modal" 
                                                                                           ng-click="tambahTahapKegiatanBlokClick(item.ID)" 
                                                                                           class="btn red btn-sm">
                                                                                            <i class="fa fa-plus"></i> Tambah Data
                                                                                        </a>
                                                                                    </div>
                                                                                </th>
                                                                            </tr>
                                                                        </thead>
                                                                        <tbody>
                                                                            <tr ng-repeat="itemdetail in dataTahapKegiatanBlok | filter: item.ID track by $index">
                                                                                <td style="min-width:190px;"> {{itemdetail.Tahap}} </td>
                                                                                <td style="min-width:240px;"> {{itemdetail.LuasDipertahankan}} </td>
                                                                                <td class="text-center">
                                                                                    <a href="javascript:void(0)" ng-click="editTahapKegiatanBlokClick(itemdetail.ID)"><i class="fa fa-pencil font-blue" title="Edit"></i></a>
                                                                                </td>
                                                                                <td class="text-center">
                                                                                    <a href="javascript:void(0)" ng-click="deleteTahapKegiatanBlok(itemdetail.ID)"><i class="fa fa-trash font-red" title="Delete"></i></a>
                                                                                </td>
                                                                            </tr>
                                                                        </tbody>
                                                                    </table>
                                                                    <!-- table detail -->
                                                                </td>
                                                                <td class="text-center">
                                                                    <a href="javascript:void(0)" ng-click="editTahapKegiatanClick(item.ID)"><i class="fa fa-pencil font-blue" title="Edit"></i></a>
                                                                </td>
                                                                <td class="text-center">
                                                                    <a href="javascript:void(0)" ng-click="deleteTahapKegiatan(item.ID)"><i class="fa fa-trash font-red" title="Delete"></i></a>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-md-12">
                                        <div class="portlet box green">
                                            <div class="portlet-title">
                                                <div class="caption">
                                                    Sumber Daya Batubara
                                                </div>
                                                <div class="actions">
                                                    <div class="btn-group btn-group-solid">
                                                        <a ng-click="tambahSumberDayaBatubaraClick()"
                                                           data-target="#static7" 
                                                           data-toggle="modal" 
                                                           class="btn red">
                                                            <i class="fa fa-plus"></i> Tambah Data
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="tools">
                                                    <a href="javascript:;" class="collapse" data-original-title="" title="" style="margin-right:5px;"> </a>
                                                </div>
                                            </div>
                                            <div class="portlet-body">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-bordered table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th style="min-width:150px;"> Blok/Area (HA) </th>
                                                                <th style="min-width:150px;"> Tereka (MT) </th>
                                                                <th style="min-width:150px;"> Terunjuk (MT) </th>
                                                                <th style="min-width:150px;"> Terukur (MT) </th>
                                                                <th style="min-width:150px;"> Total (MT) </th>
                                                                <th colspan="2"> action </th>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <tr ng-repeat="item in dataSumberDayaBatubara">
                                                                <td style="min-width:150px;"> {{item.Blok}} </td>
                                                                <td style="min-width:150px;"> {{item.Tereka}} </td>
                                                                <td style="min-width:150px;"> {{item.Terunjuk}} </td>
                                                                <td style="min-width:150px;"> {{item.Terukur}} </td>
                                                                <td style="min-width:150px;"> {{item.Tereka + item.Terunjuk + item.Terukur}} </td>
                                                                <td class="text-center">
                                                                    <a href="javascript:void(0)" ng-click="editSumberDayaBatubaraClick(item.ID)"><i class="fa fa-pencil font-blue" title="Edit"></i></a>
                                                                </td>
                                                                <td class="text-center">
                                                                    <a href="javascript:void(0)" ng-click="deleteSumberDayaBatubara(item.ID)"><i class="fa fa-trash font-red" title="Delete"></i></a>
                                                                </td>
                                                            </tr>
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                

                                <div class="row no-margin">
                                    <div class="col-md-6">
                                        <div class="portlet box green">
                                            <div class="portlet-title">
                                                <div class="caption">
                                                    Kronologi Tahapan
                                                </div>
                                                <div class="actions">
                                                    <div class="btn-group btn-group-solid">
                                                        <a data-target="#static2" 
                                                           ng-click="tambahKronologiTahapanClick()" 
                                                           data-toggle="modal"
                                                           class="btn red">
                                                            <i class="fa fa-plus"></i> Tambah Data
                                                        </a>
                                                    </div>
                                                </div>
                                                <div class="tools">
                                                    <a href="javascript:;" class="collapse" data-original-title="" title="" style="margin-right:5px;"> </a>
                                                </div>
                                            </div>
                                            <div class="portlet-body">
                                                <div class="table-responsive">
                                                    <table class="table table-striped table-bordered table-hover">
                                                        <thead>
                                                            <tr>
                                                                <th style="min-width:150px;"> Tahapan </th>
                                                                <th style="min-width:150px;"> No SK / Surat </th>
                                                                <th style="min-width:150px;"> Tanggal SK / Surat </th>
                                                                <th style="min-width:150px;"> Mulai </th>
                                                                <th style="min-width:150px;"> Sampai Dengan </th>
                                                                <th style="min-width:150px;"> Jangka Waktu </th>
                                                                <th style="min-width:150px;"> File </th>
                                                                <th style="min-width:150px;"> Keterangan </th>
                                                                <th colspan="2"> action </th>
