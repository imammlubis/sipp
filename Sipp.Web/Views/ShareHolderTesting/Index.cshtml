@model IEnumerable<EduSpot.Entity.Tables.Organization.ShareHolder>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/AngkutJual/Views/Shared/_LayoutAngkutJual.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

<table class="table">
